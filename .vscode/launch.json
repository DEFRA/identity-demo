  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Elm",
      "cwd": "${workspaceFolder}/elm",
      "program": "${workspaceFolder}/elm/index.js",
      "envFile": "${workspaceFolder}/elm/.env"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Grants",
      "cwd": "${workspaceFolder}/grants",
      "program": "${workspaceFolder}/grants/index.js",
      "envFile": "${workspaceFolder}/grants/.env"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Api",
      "cwd": "${workspaceFolder}/api",
      "program": "${workspaceFolder}/api/index.js",
      "envFile": "${workspaceFolder}/api/.env"
    }
  ]
}